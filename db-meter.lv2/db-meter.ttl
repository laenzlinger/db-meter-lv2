@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .

<https://github.com/pedalboard/db-meter.lv2>
        a lv2:Plugin ,
                lv2:AnalyserPlugin ;
        lv2:project <https://github.com/pedalboard/db-meter.lv2> ;
        doap:name "dB-Meter" ;
        doap:license <http://opensource.org/licenses/GPL-3.0-only> ;
        lv2:optionalFeature lv2:hardRTCapable ;
        lv2:port [
                a lv2:AudioPort ,
                        lv2:InputPort ;
                lv2:index 0 ;
                lv2:symbol "in" ;
                lv2:name "In"
        ] , [
                a lv2:AudioPort ,
                        lv2:OutputPort ;
                lv2:index 1 ;
                lv2:symbol "out" ;
                lv2:name "Out"
        ] , [
                a lv2:OutputPort ,
                        lv2:ControlPort ;
                lv2:index 2 ;
                lv2:symbol "level" ;
                lv2:name "Level" ,
                        "Pegel"@de ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
        ] , [
                a lv2:OutputPort ,
                        atom:AtomPort ;
                atom:bufferType atom:Sequence ;
                atom:supports midi:MidiEvent ;
                lv2:index 3 ;
                lv2:symbol "midi-level" ;
                lv2:name "Level MIDI messages"
        ] .
